n1=int(input("Digite o primeiro valor: "))
n2=int(input("Digite o segundo valor: "))
if n1>n2:
    print(f"O primeiro valor digitado foi o {n1} e é MAIOR que o segundo {n2}")
elif n2>n1:
    print(f"O segundo valor digitado foi o {n2} e é MAIOR que o primeiro {n1}")
else:
    print("Os valores são IGUAIS")
